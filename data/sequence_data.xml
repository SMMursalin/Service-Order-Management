<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="seq_service_order" model="ir.sequence">
            <field name="name">Service Order</field>
            <field name="code">service.order</field>
            <field name="prefix">SO</field>
            <field name="padding">9</field>
            <field name="company_id" eval="False"/>
        </record>

    <record id="seq_product_data" model="ir.sequence">
            <field name="name">Product Data</field>
            <field name="code">product.data</field>
            <field name="prefix">IMEI</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    <record id="seq_invoice_data" model="ir.sequence">
            <field name="name">Invoice Data</field>
            <field name="code">invoice.data</field>
            <field name="prefix">IVN</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>


</odoo>


